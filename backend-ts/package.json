{
  "name": "afm-precificacao-backend-ts",
  "version": "1.0.0",
  "description": "TypeScript backend proxy for equipment pricing API",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "build:smoke": "tsc -p tsconfig.scripts.json",
    "start": "node dist/index.js",
    "smoke:prod": "npm run build && npm run build:smoke && node dist/scripts/smoke_prod.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:embeddings": "tsx scripts/test-embeddings.ts",
    "test:embeddings:local": "tsx scripts/test-local-embeddings.ts",
    "test:factory": "tsx scripts/test-embedding-factory.ts",
    "test:search-modes": "tsx scripts/test-search-modes.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "build:dataset": "tsx src/scripts/buildDataset.ts",
    "aggregate:dataset": "tsx src/scripts/aggregateDataset.ts",
    "build:abbrev": "tsx scripts/compile-abbrev.ts",
    "validate:search": "tsx src/scripts/validateSearchV4.ts",
    "build:embeddings": "tsx scripts/buildEmbeddings.ts",
    "build:bm25": "tsx scripts/build-bm25-index.ts",
    "build:indices": "tsx scripts/build-indices.ts",
    "benchmark": "tsx scripts/benchmark.ts",
    "validate:production": "tsx scripts/validate-production.ts"
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^8.5.0",
    "@fastify/rate-limit": "^9.1.0",
    "@huggingface/transformers": "^3.8.1",
    "dotenv": "^16.4.7",
    "fastest-levenshtein": "^1.0.16",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.1",
    "lru-cache": "^11.2.4",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.28",
    "undici": "^6.19.8",
    "xlsx": "^0.18.5",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.26",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.57.0",
    "jest": "^30.2.0",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.3.3",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=22.16.0 <23.0.0"
  },
  "keywords": [
    "fastify",
    "typescript",
    "proxy",
    "api"
  ],
  "author": "",
  "license": "MIT"
}
